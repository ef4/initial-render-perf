<h1>Testing on Ember {{emberVersion}} {{#if isProdBuild}}Production{{else}}Dev{{/if}} Build</h1>

<button {{action "runOneTest"}}>
  Render Once
</button>

<button {{action "renderMany"}}>
  Render {{manyTimes}} Times
</button>

<button {{action "renderFixed"}}>
  Render For {{fixedDuration}} Seconds
</button>


<button {{action "clear"}}>
  Clear
</button>

{{#each timings as |t|}}
  <div>Render took {{t.ms}}ms</div>
{{/each}}
{{#if stats}}
  <div>Mean: {{stats.mean}}ms</div>
{{/if}}

{{#if showList}}
  {{complex-items listItems=listItems}}
{{/if}}
